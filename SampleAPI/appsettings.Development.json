{
  "PATH_BASE": "/api/v1",
  "RetriesCount": 2,
  "UseVault": false,
  "KeyVault": {
    "Url": null
  }, 
  "Logging": {
    "LogsDirectory": "c:/Logs/SampleApiLogs.txt",
    "FileOutputTemplate": "[{Timestamp:HH:mm:ss} {Level}] {SourceContext}{NewLine}{Properties:lj}{NewLine}{Message:lj}{NewLine}{Exception}{NewLine}",
    "ConsoleOutputTemplate": "[{Timestamp:HH:mm:ss} {Level}] {SourceContext}{NewLine}{Message:lj}{NewLine}{Exception}{NewLine}"
  },
  "Graylog": {
    "Enabled": false,
    "Host": "",
    "Port": 1
  },
  "JwtIssuerOptions": {
    "Issuer": "https://evbelov.au.auth0.com/",
    "Audience": "https://sample.api.dev.com"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      },
      "HttpsInlineCertFile": {
        "Url": "https://0.0.0.0:5001",
        "Certificate": {
          "Path": "./SampleApiHttps.pfx",
          "Password": "password1!",
          "AllowInvalid": "true"
        }
      }
    }
  }
}